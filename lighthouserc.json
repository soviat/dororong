{
  "ci": {
    "collect": {
      "staticDistDir": "web",
      "url": [
        "index.html",
        "market.html",
        "product_detail.html"
      ]
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "error",
          {
            "minScore": 0.9
          }
        ],
        "categories:accessibility": [
          "warn",
          {
            "minScore": 0.9
          }
        ],
        "categories:best-practices": [
          "error",
          {
            "minScore": 0.9
          }
        ],
        "categories:seo": [
          "warn",
          {
            "minScore": 0.9
          }
        ],
        "resource-summary:script": [
          "warn",
          {
            "maxNumericValue": 350000
          }
        ],
        "total-byte-weight": [
          "warn",
          {
            "maxNumericValue": 900000
          }
        ]
      }
    }
  }
}